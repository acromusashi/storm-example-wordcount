<?xml version="1.0" encoding="UTF-8"?>

<fileset-config file-format-version="1.2.0" simple-config="false" sync-formatter="false">
  <local-check-config name="storm-starter_custom CheckStyle" location="checkstyle.xml" type="project" description="">
    <additional-data name="protect-config-file" value="false"/>
  </local-check-config>
  <fileset name="storm-example-wordcount" enabled="true" check-config-name="storm-starter_custom CheckStyle" local="true">
    <file-match-pattern match-pattern="." include-pattern="false"/>
    <file-match-pattern match-pattern="storm.starter.*.java" include-pattern="true"/>
  </fileset>
  <filter name="FilesFromPackage" enabled="true">
    <filter-data value="multilang"/>
    <filter-data value="test/jvm"/>
  </filter>
</fileset-config>
